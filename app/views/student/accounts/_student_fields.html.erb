<%= f.fields_for :student_member, f.object.student_member do |ff| %>

<% p = StudentMemberFormPresenter.new(ff,self)  %>

<%= p.full_name_block( :family_name, :given_name, "氏名", disabled: true) %>

<%= p.full_name_block( :family_name_kana, :given_name_kana, "フリガナ",disabled: true) %>

<%= p.date_field_block(:birth_day , "生年月日", disabled: true) %>

<%= p.text_field_block(:grade, "学年", disabled: true) %>

<%= p.text_field_block(:a_class, "クラス", disabled: true) %>

<%= p.text_field_block(:email, "メールアドレス", size: 25, disabled: true) %>

<%= p.text_field_block(:telephone_number , "電話番号", required: true) %>

<%= p.text_field_block(:emergency_contact , "緊急連絡先", disabled: true) %>

<% end %>