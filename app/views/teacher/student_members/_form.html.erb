<h3>基本情報入力</h3>

<% p = StudentMemberFormPresenter.new(f,self)  %>

<%= p.text_field_block(:student_number, "学生番号", requied: true) %>

<%= p.full_name_block( :family_name, :given_name, "氏名",) %>

<%= p.full_name_block( :family_name_kana, :given_name_kana, "フリガナ",) %>

<%= p.text_field_block(:emergency_contact, "緊急連絡先", requied: true) %>

<%= p.date_field_block(:start_date, "入学日", required:true) %>

<%= p.text_field_block(:grade, "学年", requied: true) %>

<%= p.text_field_block(:a_class, "クラス", requied: true) %>

<%= p.suspended_check_box %>