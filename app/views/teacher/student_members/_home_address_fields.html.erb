<%= f.fields_for :home_address, f.object.student_member.home_address do |ff| %>

<% p = AddressFormPresenter.new(ff,self) %>

<%= p.postal_code_block(:postal_code, "郵便番号", size: 7) %>

<%= p.drop_down_list_block(:prefecture, "都道府県", StudentMemberAddress::PREFECTURE_NAMES) %>

<%= p.text_field_block(:city, "市区町村", class: "form-control") %>

<%= p.text_field_block(:address1, "町域、番地等", class: "form-control") %>

<%= p.text_field_block(:address2, "建物名、部屋番号等", class: "form-control") %>

<% end %>